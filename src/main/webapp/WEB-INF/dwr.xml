<!DOCTYPE dwr PUBLIC
        "-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
        "http://getahead.org/dwr/dwr30.dtd">

<dwr>
    <allow>
        <create creator="new" javascript="PrefixService">
            <param name="class" value="com.mednet.service.PrefixService"/>
        </create>

        <convert converter="bean" match="com.mednet.model.Prefix"/>
        <convert converter="exception" match="java.lang.Exception"/>
        <convert converter="exception" match="java.lang.RuntimeException"/>
        <convert converter="exception" match="java.lang.Throwable"/>

    </allow>

</dwr>
